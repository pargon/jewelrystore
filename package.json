    "eslint": "^8.57.1",
    "prettier-eslint": "^16.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "react-scripts": "^5.0.1",
    "styled-components": "^6.1.19",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
